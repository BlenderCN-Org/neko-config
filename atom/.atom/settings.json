{
	"activate-power-mode": {
		"autoToggle": false
	},
	"atom-beautify": {
		"cpp": {
			"beautify_on_save": true,
			"configPath": "~/Nekocloud/Projects/neko-config/uncrustify/uncrustify.cfg"
		},
		"cs": {
			"beautify_on_save": true,
			"configPath": "C:\\Program Files (x86)\\Uncrustify\\cfg\\defaults.cfg"
		},
		"css": {
			"preserve_newlines": true
		},
		"elm": {
			"beautify_on_save": true
		},
		"general": {
			"_analyticsUserId": "83234c70-1d1a-424d-ab10-205e6b54991b"
		},
		"html": {
			"beautify_on_save": true
		},
		"java": {
			"beautify_on_save": true,
			"configPath": "~/Nekocloud/Shared/neko-config/config/uncrustify.cfg"
		},
		"js": {
			"end_with_newline": true
		},
		"json": {
			"beautify_on_save": true
		},
		"php": {
			"beautify_on_save": true,
			"default_beautifier": "PHPCBF",
			"standard": "~/Nekocloud/Shared/github/neko-config/config/phpcbf.xml"
		},
		"python": {
			"beautify_on_save": true,
			"ignore": [
				"E501"
			]
		},
		"ruby": {
			"beautify_on_save": true,
			"default_beautifier": "Ruby Beautify"
		},
		"rust": {
			"beautify_on_save": true
		},
		"sass": {
			"beautify_on_save": true
		},
		"scss": {
			"convert_quotes": "single",
			"preserve_newlines": true
		},
		"vue": {
			"beautify_on_save": true,
			"end_with_comma": true,
			"end_with_newline": true,
			"eval_code": true,
			"keep_array_indentation": true,
			"keep_function_indentation": true
		},
		"xml": {
			"beautify_on_save": true
		}
	},
	"atom-clock": {
		"dateFormat": "dddd D/M/Y - HH:mm",
		"showClockIcon": true,
		"showTooltip": true
	},
	"atom-ide-ui": {
		"use": {
			"atom-ide-diagnostics-ui": "never",
			"atom-ide-terminal": "always"
		}
	},
	"atom-maven": {
		"mavenHome": "C:\\Program Files\\Maven\\bin"
	},
	"atom-package-deps": {
		"ignored": [
			"java-plus"
		]
	},
	"autocomplete-python": {
		"useKite": false
	},
	"city-lights-ui": {
		"fontSize": 10,
		"tabSize": "small"
	},
	"core": {
		"autoHideMenuBar": true,
		"disabledPackages": [
			"python-tools",
			"spell-check",
			"markdown-preview",
			"haxe",
			"autocomplete-ruby",
			"battery-status",
			"atom-autocomplete-php"
		],
		"ignoredNames": [
			".git",
			".hg",
			".svn",
			".DS_Store",
			"._*",
			"Thumbs.db",
			"desktop.ini",
			".pioenvs",
			".piolibdeps",
			".clang_complete",
			".gcc-flags.json"
		],
		"telemetryConsent": "limited",
		"themes": [
			"seti-ui",
			"seti-classic-syntax"
		],
		"titleBar": "custom",
		"uriHandlerRegistration": "always",
		"useTreeSitterParsers": true
	},
	"document-outline": {
		"showByDefault": true
	},
	"double-tag": {
		"allowEndTagSync": true,
		"enabledScopes": [
			"text.html",
			"text.html.basic",
			"text.xml",
			"text.marko",
			"source.js.jsx",
			"source.tsx",
			"text.html.erb",
			"text.html.php",
			"text.html.php.blade",
			"text.html.vue",
			"meta.tag.other.html"
		]
	},
	"editor": {
		"fontSize": 11,
		"showIndentGuide": true,
		"tabLength": 4,
		"tabType": "soft"
	},
	"editor-background": {
		"box3d": {
			"shadowOpacity": 0
		},
		"image": {
			"opacity": 50,
			"repeat": "repeat",
			"url": "https://github.com/catlinman/neko-config/raw/master/hexchat/config/bg.png"
		},
		"other": {
			"transparentTabBar": false
		},
		"text": {
			"blur": 0,
			"opacity": 0
		}
	},
	"exception-reporting": {
		"userId": "3a952176-b4d2-2770-c3df-2eab055dc8cd"
	},
	"figlet": {
		"defaultFont": "Univers"
	},
	"file-icons": {
		"strategies": {}
	},
	"fonts": {
		"fontFamily": "Inconsolata"
	},
	"git-plus": {
		"general": {
			"_analyticsUserId": "5c02ae9a-f631-4f52-803c-be7e7126bd04"
		}
	},
	"ide-python": {
		"pylsPlugins": {
			"pydocstyle": {
				"convention": "pep257",
				"enabled": true
			}
		}
	},
	"line-ending-selector": {
		"defaultLineEnding": "LF"
	},
	"linter-eslint": {
		"disableWhenNoEslintConfig": false,
		"fixOnSave": true
	},
	"linter-gcc": {
		"gccDefaultCppFlags": "-c -Wall -std=c++11 -fsyntax-only"
	},
	"linter-js-standard": {
		"honorStyleSettings": false,
		"lintHtmlFiles": true
	},
	"linter-jshint": {
		"lintInlineJavaScript": true,
		"scopes": [
			"source.js",
			"source.js-semantic",
			"javascript"
		]
	},
	"linter-pep8": {
		"ignoreErrorCodes": [
			"E501"
		]
	},
	"linter-pycodestyle": {
		"ignoreErrorCodes": [
			"E501"
		]
	},
	"linter-scss-lint": {
		"additionalArguments": "-x Indentation"
	},
	"linter-spell": {
		"markdownWords": [
			"mysql",
			"irc",
			"hexchat"
		],
		"spellPath": "aspell"
	},
	"linter-stylelint": {
		"disableWhenNoConfig": false,
		"useStandard": true
	},
	"linter-ui-default": {
		"panelHeight": 146,
		"showPanel": true,
		"useBusySignal": false
	},
	"linter-xo": {
		"fixOnSave": true
	},
	"minimap": {
		"plugins": {
			"codeglance": true,
			"codeglanceDecorationsZIndex": 0,
			"cursorline": true,
			"cursorlineDecorationsZIndex": 0,
			"find-and-replace": true,
			"find-and-replaceDecorationsZIndex": 0,
			"git-diff": true,
			"git-diffDecorationsZIndex": 0,
			"highlight-selected": true,
			"highlight-selectedDecorationsZIndex": 0,
			"linter": true,
			"linterDecorationsZIndex": 0,
			"pigments": true,
			"pigmentsDecorationsZIndex": 0
		}
	},
	"nekoatom": {
		"compactView": true
	},
	"pigments": {
		"extendAutocompleteToColorValue": true,
		"extendAutocompleteToVariables": true
	},
	"platformio-ide-terminal": {
		"style": {
			"fontSize": "10"
		}
	},
	"processing-preview": {
		"liveUpdate": false
	},
	"save-session": {
		"dataSaveFolder": "/Users/catlinman/.atom/packages/save-session/projects"
	},
	"seti-ui": {
		"compactView": true,
		"disableAnimations": true
	},
	"spell-check": {
		"grammars": [
			"source.asciidoc",
			"source.gfm",
			"text.git-commit",
			"text.plain",
			"text.plain.null-grammar",
			"text.html.php",
			"source.python",
			"source.moon",
			"text.html.basic",
			"source.java"
		]
	},
	"split-diff": {
		"diffWords": true,
		"ignoreWhitespace": true,
		"leftEditorColor": "red",
		"rightEditorColor": "green",
		"scrollSyncType": "Vertical + Horizontal",
		"syncHorizontalScroll": true
	},
	"symbols-tree-view": {
		"autoHide": true,
		"autoToggle": true,
		"defaultWidth": 300
	},
	"sync-settings": {
		"quietUpdateCheck": true,
		"removeObsoletePackages": true
	},
	"terminal-plus": {
		"core": {
			"mapTerminalsTo": "Folder",
			"shell": "C:\\Windows\\System32\\cmd.exe"
		}
	},
	"tool-bar": {
		"iconSize": "12px",
		"position": "Right"
	},
	"tree-view": {
		"hideIgnoredNames": true,
		"hideVcsIgnoredFiles": true
	},
	"welcome": {
		"showOnStartup": false
	}
}